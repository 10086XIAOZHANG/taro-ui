{
  "extends": ["taro"],
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "Taro" }],
    "quotes": [2, "single"],
    "react/self-closing-comp": "off",
    "semi": [2, "never"],
    "react/jsx-boolean-value": "off",
    "react/jsx-closing-bracket-location": [
      "error",
      {
        "nonEmpty": "after-props",
        "selfClosing": "after-props"
      }
    ]
  },
  "parser": "babel-eslint"
}
